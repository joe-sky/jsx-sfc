{
  "name": "vscode-jsx-sfc",
  "displayName": "jsx-sfc",
  "description": "Useful editor tools for jsx-sfc.",
  "version": "0.3.0",
  "publisher": "joe-sky",
  "icon": "images/react-logo.png",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "jsx-sfc",
      "properties": {
        "jsx-sfc.icon.splitEditors": {
          "type": "boolean",
          "default": true,
          "description": "Show split editor icon in title area of editor."
        }
      }
    },
    "commands": [
      {
        "command": "jsx-sfc.action.splitEditors",
        "title": "Split Component(), render(), styles() Editors",
        "category": "jsx-sfc",
        "icon": "images/split-editors.png"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "jsx-sfc.action.splitEditors",
          "when": "editorLangId == javascript && config.jsx-sfc.icon.splitEditors",
          "group": "navigation"
        },
        {
          "command": "jsx-sfc.action.splitEditors",
          "when": "editorLangId == javascriptreact && config.jsx-sfc.icon.splitEditors",
          "group": "navigation"
        },
        {
          "command": "jsx-sfc.action.splitEditors",
          "when": "editorLangId == typescriptreact && config.jsx-sfc.icon.splitEditors",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest --config=test/jest.config.js",
    "jest:clear": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-sky/jsx-sfc.git"
  },
  "keywords": [
    "jsx-sfc",
    "sfc",
    "react",
    "single file component"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.171",
    "@types/node": "^7.0.43",
    "@types/vscode": "^1.56.0",
    "jest": "^27.0.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-json-strings": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-syntax-decorators": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@vue/reactivity": "^3.0.11",
    "babel-plugin-jsx-sfc": "^1.5.1",
    "lodash": "^4.17.21"
  }
}
