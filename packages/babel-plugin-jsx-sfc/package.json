{
  "name": "babel-plugin-jsx-sfc",
  "version": "1.6.1",
  "description": "Babel plugin for compile and enhance the limitation of jsx-sfc runtime code.",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lib": "babel src --out-dir lib --extensions \".js,.jsx,.ts,.tsx\" --config-file \"../../.babelrc\"",
    "dts": "rollup -c -o index.d.ts",
    "build": "yarn run lib && yarn run dts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-sky/jsx-sfc.git"
  },
  "bugs": {
    "url": "https://github.com/joe-sky/jsx-sfc/issues"
  },
  "author": "Joe_Sky <hitomi_doax@sina.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-react-jsx": "^7.7.5"
  }
}
